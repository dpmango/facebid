  "devDependencies": {},
  "scripts": {
    "start-scripts": "HTTPS=true node scripts/start.js",
    "build-scripts": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "start": "concurrently --kill-others \"gulp\" \"yarn start-scripts\" \"yarn json-server\"",
    "json-server": "nodemon api/server.js",
    "json-server-pure": "json-server --watch api/db.json -p 8000",
    "social-server": "nodemon social-api/server.js",
    "build": "gulp build --production && yarn build-scripts",
    "deploy-json-server": "git subtree push --prefix api heroku master",
    "deploy-social-server": "git subtree push --prefix social-api heroku-social master",
    "deploy-all": "mv build/index.html build/200.html && surge --domain facebid.surge.sh --project ./build && yarn deploy-json-server"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
